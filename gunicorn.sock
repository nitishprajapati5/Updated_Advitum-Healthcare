[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/Advitum_HealthCare
ExecStart=/home/ubuntu/Advitum_HealthCare/venv/bin/gunicorn \
          --workers 3 \
          --bind unix:/home/ubuntu/Advitum_HealthCare/gunicorn.sock \
          agro.wsgi:application

[Install]
WantedBy=multi-user.target